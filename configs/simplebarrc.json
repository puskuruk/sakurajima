    "showCurrentSpaceMode": true,
    "hideWindowTitle": false,
    "displayOnlyCurrent": true,
    "expandAllProcesses": false,
    "displaySkhdMode": false,
    "displayStackIndex": true,
    "displayOnlyIcon": false
  },
  "spacesDisplay": {
    "exclusions": "",
    "titleExclusions": "",
    "spacesExclusions": "",

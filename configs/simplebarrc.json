  "dateWidgetOptions": {
    "refreshFrequency": 30000,
    "shortDateFormat": true,
    "locale": "en-US",
    "calendarApp": ""
  },
  "timeWidgetOptions": {
    "refreshFrequency": 1000,
    "hour12": true,
    "dayProgress": false,
    "showSeconds": false
  },
  "userWidgetOptions": {
    "refreshFrequency": false
  },
  "customStyles": {
    "styles": "/* Sakurajima simple-bar custom styles */\n.simple-bar {\n  font-family: 'JetBrains Mono', 'SF Pro', monospace;\n}\n\n.simple-bar--spaces .space {\n  font-weight: 500;\n}\n\n.simple-bar--spaces .space--focused {\n  font-weight: 700;\n}\n"
  }
